<style>
  :where(.sign) {
    --color-sign: #5e81ac;

    transition: fill, 0.2s;

    fill: transparent;
    stroke-width: 16px;
    stroke-linejoin: round;

    stroke: var(--color-sign);
    stroke-dasharray: var(--total-length) var(--total-length);
    stroke-dashoffset: calc(var(--total-length) * -1);

    &[data-part='qin'] {
      --total-length: 6660;
      animation: signature 2s linear forwards;
    }

    &[data-part='dot'] {
      --color-sign: #a3be8c;
      --total-length: 502;
      animation: signature 0.5s linear 2s forwards;
    }

    &[data-part='shaner'] {
      --total-length: 9802;
      animation: signature 2s linear 2.5s forwards;
    }
  }

  :where(html[class*='dark'] .sign) {
    --color-sign: #81a1c1;
  }

  @keyframes signature {
    90% {
      fill: transparent;
      stroke-width: 16px;
    }

    100% {
      fill: var(--color-sign);
      stroke-dashoffset: 0;
      stroke-width: 0px;
    }
  }
</style>
